{
  "name": "trello-iqb",
  "version": "0.1.0",
  "description": "Input Quality Bar for Trello board tasks",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha && npm run lint && npm run format",
    "format-fix": "npx prettier . --write",
    "format": "npx prettier . --check",
    "lint-fix": "npx eslint . --fix",
    "lint": "npx eslint . --max-warnings=0"
  },
  "keywords": [
    "Trello",
    "Project",
    "Collaboration",
    "Kanban",
    "Cards"
  ],
  "author": "Borys Lebeda",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "trello": "^0.11.0"
  },
  "engines": {
    "node": ">=14",
    "vscode": ">=1.22.0",
    "npm": ">=8.1.0"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "mocha": "^9.1.3"
  }
}
